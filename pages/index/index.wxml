<view>{{title}}</view>
<swiper indicator-dots="{{indicatorDots}}"
  autoplay="{{autoplay}}" interval="{{interval}}">
  <block wx:for="{{imgUrls}}">
    <swiper-item>
      <image src="{{item}}" class="slide-image" width="100%" height="100%"/>
    </swiper-item>
  </block>
</swiper>

<progress percent="{{progress}}" show-info />